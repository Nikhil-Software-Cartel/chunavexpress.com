{
	"name": "frontend",
	"version": "1.0.0",
	"scripts": {
		"rdeploy": "pnpm sync-env && ssh bbp \" source ~/.nvm/nvm.sh && cd chunavexpress.com/frontend/ && git pull && pnpm i && pnpm build && pnpm start \"",
		"sync-env": "scp .env bbp:chunavexpress.com/frontend/.env",
		"build": "vite build",
		"serve:dev": "vite --config vite.config.js",
		"prod": "vite preview",
		"start": "pm2 start ecosystem.config.cjs",
		".copytoremote": "scp -r build/ bbp:chunavexpress.com/frontend/build/ && scp -r ./package.json bbp:chunavexpress.com/frontend/",
		"ls": "dir",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^4.5.10",
		"@lexical/html": "^0.12.2",
		"@lexical/utils": "^0.12.2",
		"@neoconfetti/svelte": "^1.0.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.20.4",
		"@sveltejs/vite-plugin-svelte": "^2.4.6",
		"@types/cookie": "^0.5.1",
		"less": "^4.2.0",
		"lexical": "^0.12.2",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"scss": "^0.2.4",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"svelte-preprocess": "^5.0.4",
		"typescript": "^5.0.0",
		"vite": "^4.4.2",
		"vite-plugin-purgecss": "^0.2.12"
	},
	"type": "module",
	"dependencies": {
		"dotenv": "^16.3.1",
		"swiper": "^10.3.1"
	}
}
